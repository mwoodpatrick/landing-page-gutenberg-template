{
	"presets": [
		[
		  "@babel/preset-env",
		  {
			"targets": {
			  "esmodules": true
			}
		  }
		],
		["@babel/preset-react"]
	  ],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-transform-arrow-functions",
		[
			"minify-mangle-names",
			{ "exclude": { "__": true, "_n": true, "_x": true, "_nx": true } }
		]
	]
}
